<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list android:layout_width="wrap_content" android:layout_height="wrap_content">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/cordefundo"></solid>
                    <corners android:radius="15dp"></corners>
                    <padding android:right="5dp"></padding>
                    <stroke android:color="@color/black" android:width="4dp"/>
                </shape>
            </item>
            <item>
                <bitmap android:gravity="center|end" android:src="@drawable/ic_action_up"></bitmap>
            </item>
        </layer-list>
    </item>
</selector>